{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": 0,
        "no-console": 0,
        "no-param-reassign": 0,
        "no-underscore-dangle": 0
    }
}